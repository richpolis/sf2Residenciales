<div class="row">
    <div class="col-md-12">
        <div class="widget">
            <div class="widget-head">
                <div class="pull-left">Registro de {{stringTipoCategoria|capitalize}}</div>
                <div class="clearfix"></div>
            </div>
            <div class="widget-content">
                <table id="{{stringTipoCategoria}}" class="tipo-categoria-{{tipoCategoria}} table table-striped table-bordered table-hover">
                    <thead>
                        <tr>
                            <th>Categoria</th>
                            <th>Imagen</th>
                            <th>Titulo</th>
                            <th>Es principal</th>
                            <th>En carrusel</th>
                            <th>Usuario</th>
                            <th>Estatus</th>
                            <th>Actions</th>
                        </tr>
                    </thead>
                    <tbody class="cuerpoTabla">
                        {% for entity in entities %}
                            {% include 'BackendBundle:Default:item.html.twig' with {'entity': entity } %}
                        {% endfor %}
                    </tbody>
                </table>
                <div class="widget-foot">
                    <br><br>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
    </div>
</div>